    },
    {
      "start_time": 12,
      "end_time": 15,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Only the monstrous anger of the guns."
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Transcription",
          "category": "Background"
        },
        {
          "tag": "Tempo",
          "category": "Performance"
        },
        {
          "tag": "Amplitude",
          "category": "Performance"
        }
      ],
      "uuid": "a4e8e8af-b3e7-40d6-b755-dd04eb26f603"
    },
    {
      "start_time": 16,
      "end_time": 18,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Only the stuttering rifles' rapid rattle"
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Transcription",
          "category": "Background"
        },
        {
          "tag": "Rhyme",
          "category": "Form"
        }
      ],
      "uuid": "3fc78226-fca7-4c68-9c22-b83f34332665"
    },
    {
      "start_time": 19,
      "end_time": 21,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Can patter out their hasty orisons."
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Transcription",
          "category": "Background"
        },
        {
          "tag": "Rhyme",
          "category": "Form"
        }
      ],
      "uuid": "6d8ac4f3-84b1-4261-b71b-333bbc24a7ae"
    },
    {
      "start_time": 20,
      "end_time": 20,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Orison - A prayer"
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Definition",
          "category": "Background"
        }
      ],
      "uuid": "f01f414e-0bfc-4d55-b4c5-75512dd211d3"
    },
    {
      "start_time": 21,
      "end_time": 24,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "No mockeries now for them; no prayers nor bells;"
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Transcription",
          "category": "Background"
        },
        {
          "tag": "Amplitude",
          "category": "Performance"
        }
      ],
      "uuid": "16947134-c4cc-4ed3-b125-7249087bf5b5"
    },
    {
      "start_time": 25,
      "end_time": 27,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Nor any voice of mourning save the choirs,—"
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Transcription",
          "category": "Background"
        }
      ],
      "uuid": "c0ea6b9a-137f-4f45-b1e4-c3048ccb43d2"
    },
