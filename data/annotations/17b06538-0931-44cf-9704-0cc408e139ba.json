      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Gunfire & artillery becomes oppressive, before fading out slightly."
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Ambient Noise",
          "category": "Extra-Poetic Sounds"
        }
