      "start_time": 34,
      "end_time": 37,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "And bugles calling for them from sad shires."
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Transcription",
          "category": "Background"
        },
        {
          "tag": "Rhyme",
          "category": "Form"
        }
